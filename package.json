{
  "name": "payload-ab",
  "version": "0.2.4",
  "description": "Payload CMS plugin for A/B testing with PostHog",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./server": {
      "import": "./dist/exports/server.js",
      "types": "./dist/exports/server.d.ts",
      "default": "./dist/exports/server.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "payload",
    "payload-plugin",
    "payload-cms",
    "a/b-testing",
    "posthog",
    "analytics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brijr/payload-ab.git"
  },
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "dev": "payload run ./dev/server.ts",
    "dev:generate-importmap": "pnpm dev:payload generate:importmap",
    "dev:generate-types": "pnpm dev:payload generate:types",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=./dev/payload.config.ts payload",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@payloadcms/db-mongodb": "3.29.0",
    "@payloadcms/db-postgres": "3.29.0",
    "@payloadcms/db-sqlite": "3.29.0",
    "@payloadcms/eslint-config": "3.9.0",
    "@payloadcms/next": "3.29.0",
    "@payloadcms/richtext-lexical": "3.29.0",
    "@payloadcms/ui": "3.29.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@swc/jest": "^0.2.39",
    "@types/jest": "29.5.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.16.3",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.2.3",
    "graphql": "^16.11.0",
    "jest": "29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "next": "15.2.3",
    "open": "^10.1.2",
    "payload": "3.29.0",
    "prettier": "^3.6.2",
    "qs-esm": "7.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rimraf": "3.0.2",
    "sharp": "0.32.6",
    "sort-package-json": "^2.15.1",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "payload": "^3.29.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": ">=9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "registry": "https://registry.npmjs.org/",
  "dependencies": {
    "lodash.merge": "^4.6.2",
    "posthog-node": "^5.6.0"
  },
  "packageManager": "pnpm@10.12.4"
}
